public class Duplicates{
    public static void main(String args[]){
        int a[]={10,20,30,30,50},i,j,flag=0;
        for(i=0;i<a.length;i++){
            for(j=0;j<i;j++){
                if(a[i]==a[j]){
                    flag=1;
                    break;
                }
            }
        }
        if(flag==1)
        System.out.println("duplicates found");
        else
        System.out.println("duplicates not found");
    }
}
