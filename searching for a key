public class LinearSearch{
    public static void main(String args[]){
        int a[]={10,20,30,40,50};
        int i;
        int key=30;
        int flag=0;
        for(i=0;i<a.length;i++)
        {
            if(a[i]==key){
                flag=1;
                break;
            }
        
        }
        if(flag==1)
            System.out.println(key+" is at index "+i);
        else
            System.out.println("key not found");
    }
}
